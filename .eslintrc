{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unsafe-finally": 2,
    "no-unreachable": 2,
    "no-unexpected-multiline": 2,
    "valid-typeof": 2,
    "no-undefined": 2,
    "strict": 0,
    "no-console": 0,
    "no-alert": 2,
    "no-with": 2,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-debugger": 2,
    "no-empty": 2,
    "no-extra-semi": 2,
    "no-irregular-whitespace": 2,
    "dot-location": 0,
    "dot-notation": 1,
    "no-floating-decimal": 2,
    "eqeqeq": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-redeclare": 2,
    "no-script-url": 2,
    "radix": 2,
    "indent": 2,
    "max-len": 0,
    "no-mixed-spaces-and-tabs": 2,
    "init-declarations": 2,
    "no-delete-var": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-sparse-arrays": 2,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-ex-assign": 2,
    "use-isnan": 2,
    "no-extra-boolean-cast": 2,
    "no-func-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-inner-declarations": 2,
    "no-obj-calls": 1,
    "func-call-spacing": 2,
    "no-control-regex": 2,
    "no-empty-character-class": 2,
    "no-invalid-regexp": 2,
    "no-regex-spaces": 2
  }
}
